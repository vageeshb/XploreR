<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service name="DSOD_Assignment2.bankService">
                <endpoint address="" binding="basicHttpBinding" contract="DSOD_Assignment2.IbankService">
                    <identity>
                        <dns value="localhost" />
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/DSOD_Assignment2/bankService/" />
                    </baseAddresses>
                </host>
            </service>
            <service name="DSOD_Assignment2.demo">
                <endpoint address="" binding="basicHttpBinding" contract="DSOD_Assignment2.Idemo">
                    <identity>
                        <dns value="localhost" />
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/DSOD_Assignment2/demo/" />
                    </baseAddresses>
                </host>
            </service>
        </services>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IService1" />
                <binding name="BasicHttpBinding_IService" />
                <binding name="BasicHttpBinding_IService11" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:10215/Service1.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IService1" contract="BankReference.IService1"
                name="BasicHttpBinding_IService1" />
            <endpoint address="http://venus.eas.asu.edu/WSRepository/Services/EncryptionWcf/Service.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IService"
                contract="encryptReference.IService" name="BasicHttpBinding_IService" />
            <endpoint address="http://localhost:10215/Service1.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IService11" contract="BankRef.IService1"
                name="BasicHttpBinding_IService11" />
        </client>
    </system.serviceModel>
</configuration>